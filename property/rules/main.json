{
  "rules": {
    "name": "default",
    "children": [
      "#include:offload.json",
      "#include:performance.json",
      "#include:redirects.json",
      "#include:security.json"
    ],
    "behaviors": [
      {
        "name": "origin",
        "options": {
          "cacheKeyHostname": "ORIGIN_HOSTNAME",
          "compress": true,
          "customValidCnValues": [
            "{{Origin Hostname}}",
            "{{Forward Host Header}}"
          ],
          "enableTrueClientIp": true,
          "forwardHostHeader": "ORIGIN_HOSTNAME",
          "hostname": "${env.originHostname}",
          "httpPort": 80,
          "httpsPort": 443,
          "ipVersion": "IPV4",
          "minTlsVersion": "DYNAMIC",
          "originCertsToHonor": "COMBO",
          "originSni": true,
          "originType": "CUSTOMER",
          "standardCertificateAuthorities": [
            "akamai-permissive",
            "THIRD_PARTY_AMAZON"
          ],
          "tls13Support": false,
          "verificationMode": "CUSTOM",
          "trueClientIpHeader": "True-Client-IP",
          "trueClientIpClientSetting": false,
          "customCertificateAuthorities": [],
          "customCertificates": []
        }
      },
      {
        "name": "cpCode",
        "options": {
          "value": {
            "id": "${env.cpCode}"
          }
        }
      },
      {
        "name": "report",
        "options": {
          "logHost": true,
          "logReferer": true,
          "logUserAgent": true,
          "logAcceptLanguage": true,
          "logCookies": "OFF",
          "logCustomLogField": false,
          "logEdgeIP": true,
          "logXForwardedFor": true
        }
      }
    ],
    "options": {
      "is_secure": false
    },
    "variables": [],
    "comments": "The behaviors in the Default Rule apply to all requests for the property hostname(s) unless another rule overrides the Default Rule settings."
  },
  "comments": "Initial configuration."
}
